<component-set>
    <components>
        <component>
            <role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
            <role-hint>ccl</role-hint>
            <implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping</implementation>
            <configuration>
                <lifecycles>
                    <lifecycle>
                        <id>default</id>
                        <phases>
                            <process-resources>com.cerner.ccltesting:cerner-maven-ccl-plugin:process-resources</process-resources>
                            <compile>com.cerner.ccltesting:cerner-maven-ccl-plugin:compile</compile>
                            <process-test-resources>com.cerner.ccltesting:cerner-maven-ccl-plugin:process-test-resources</process-test-resources>
                            <test-compile>com.cerner.ccltesting:cerner-maven-ccl-plugin:test-compile</test-compile>
                            <test>com.cerner.ccltesting:cerner-maven-ccl-plugin:test</test>
                        </phases>
                    </lifecycle>
                </lifecycles>
            </configuration>
        </component>
        <component>
            <role>org.sonatype.plexus.components.sec.dispatcher.SecDispatcher</role>
            <role-hint>mng-4384</role-hint>
            <implementation>hidden.org.sonatype.plexus.components.sec.dispatcher.DefaultSecDispatcher</implementation>
            <description></description>
            <requirements>
                <requirement>
                    <role>org.sonatype.plexus.components.cipher.PlexusCipher</role>
                    <role-hint>mng-4384</role-hint>
                    <field-name>_cipher</field-name>
                </requirement>
                <requirement>
                    <role>org.sonatype.plexus.components.sec.dispatcher.PasswordDecryptor</role>
                    <role-hint>mng-4384</role-hint>
                    <field-name>_decryptors</field-name>
                </requirement>
            </requirements>
            <configuration>
                <_configuration-file>~/.m2/settings-security.xml</_configuration-file>
            </configuration>
        </component>
        <component>
            <role>org.sonatype.plexus.components.cipher.PlexusCipher</role>
            <role-hint>mng-4384</role-hint>
            <implementation>hidden.org.sonatype.plexus.components.cipher.DefaultPlexusCipher</implementation>
            <description></description>
        </component>
    </components>
</component-set>